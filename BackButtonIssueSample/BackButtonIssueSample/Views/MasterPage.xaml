<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="BackButtonIssueSample.Views.MasterPage"
                  xmlns:pages="clr-namespace:BackButtonIssueSample.Views">
  
  <MasterDetailPage.Master>
    <pages:MasterPageMaster x:Name="MasterPageName" />
  </MasterDetailPage.Master>
  
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MasterPageDetail />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>